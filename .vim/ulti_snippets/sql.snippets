endsnippet

snippet cre
CREATE TABLE ${1:table_name} (
  id INT UNSIGNED AUTO_INCREMENT,
  PRIMARY KEY (id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
endsnippet

snippet id
id INT UNSIGNED AUTO_INCREMENT,
endsnippet

snippet pri
PRIMARY KEY (id)
endsnippet

snippet uni
UNIQUE KEY index_${1:table_name}_on_${2:column} (${2})
endsnippet

snippet fk
CONSTRAINT ${1:thisTableName} ${2:otherTableName} fk FOREIGN KEY (${3:ThisColumnName}) REFERENCES $2 (id)
endsnippet

