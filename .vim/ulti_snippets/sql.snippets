snippet proc
-- Notes: ${1}
DROP PROCEDURE IF EXISTS sp${2:ProcName}//
CREATE PROCEDURE sp$2(
  IN inNumClientIp INTEGER UNSIGNED,
  IN inStrSid VARCHAR (31),
  ${3},
  OUT outStrError varchar(255)
)
DETERMINISTIC
proc:BEGIN
     SET outStrError = 'sp$2: error';
  ${4}

  CALL spInsertUserAction(inNumClientIp, inStrSid, '$2', '', true);

     SET outStrError = 'sp$2: success';
END// -- _sp$2
endsnippet

snippet log
CALL spInsertUserAction(inNumClientIp, inStrSid, 'sp${1}', '', true);
